<seelog>
  <outputs formatid="common">
    <filter levels="trace,debug,info,warn,error,critical">
      <file path="log/timeserver.log" />
      <console />
    </filter>
  </outputs>
  <formats>
    <format id="common" format="%Date-%Time%t[%LEVEL]%t'%Msg'%t%Func:%Line%t%t%n" />
  </formats>
</seelog>